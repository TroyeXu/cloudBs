                            tr
                                th(scope='col') 順序
                                th(scope='col') 天氣狀況
                                th(scope='col') 溫度
                                th(scope='col') 濕度
                        tbody
                            tr
                                th(scope='row') 1
                                td
                                    i.fas.fa-tint.text-primary.fa-2x.ml-2
                                td 2℃ 
                                td 90.4%
                            tr
                                th(scope='row') 2
                                td
                                    i.fas.fa-sun.text-warning.fa-2x.ml-2
                                td 12℃ 
                                td 8%
                            tr
                                th(scope='row') 3
                                td 
                                    i.fas.fa-tint.text-primary.fa-2x.ml-2
                                td 8℃ 
                                td 7%
