                    .circle.halfcircle1.rounded-circle
                    .circle.halfcircle2.rounded-circle
                    .circle.halfcircle3.rounded-circle                    
                .d_bottom
                    table.table.table-hover.table-dark(style="  background-color: #192e56c2")
