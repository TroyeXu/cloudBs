.container
    .row
        .col-md-5
            .daybox
                .d_top
                    .d_data.py-3.px-3
                        .h3.location.mb-3 Taichung
                        .wet.mb-3 90.4
                        .cloud.mb-3
                            .rain1
                            .rain2
                            .rain3
                        .h2.mb-3.temperature.font-weight-bold 12℃ 
